#include <iostream>  
#include <vector>    
 using namespace std ;
void bubbleSort( vector<int>& arr) {
    int n = arr.size(); 

    for (int i = 0; i < n - 1; ++i) {
         
        for (int j = 0; j < n - i - 1; ++j) {
   
            if (arr[j] > arr[j + 1]) {
                
              swap(arr[j], arr[j + 1]);
            }
        }
    }
}

  
void printArray(const vector<int>& arr) {
    for (int x : arr) {
       cout << x << " ";
    }
   cout << endl;
}

int main() {
     vector<int> myVector = {24, 16, 8, 5, 30, 11, 90};

   cout << "Original array: ";
    printArray(myVector);

    bubbleSort(myVector);  

   cout << "Sorted array: ";
    printArray(myVector);

    return 0;
}
